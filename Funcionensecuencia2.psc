Algoritmo Funcionensecuencia2
    Definir x Como Entero
    Definir div Como Real
    Escribir "Ingrese número: "
    Leer x
    x=g(x)
	Escribir "Resultado func g: ",x
	div=f(x)
    Escribir "Resultado func f: ",div
FinAlgoritmo

Funcion res <- g(n)
	Definir res Como Entero
    res <- n - 3
FinFuncion

Funcion res <- f(n)
	Definir res Como real
    res <- n / 2
FinFuncion